/*******************************
         Theme Overrides
*******************************/

.ui.menu {
  border-radius: 0;
  border: 0;
  box-shadow: none;
}

.ui.menu:after {
  display: none;
}

.ui.vertical.menu {
  box-shadow: none;
  width: 100%;
  padding: 20px 5px;
}

.ui.vertical.menu .item.label > i.icon {
  float: left;
}
